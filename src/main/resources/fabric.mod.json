{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "Enhances your Minecraft experience with 16 vibrant new dyes and various other tweaks",
  "authors": [
    "Yapetto",
    "Ninni"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/dye_depot",
    "issues": "https://github.com/${repository}/issues",
    "sources": "https://github.com/${repository}"
  },
  "license": "CC0-1.0",
  "accessWidener": "${mod_id}.accesswidener",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ninni.dye_depot.DyeDepot"
    ],
    "client": [
      "com.ninni.dye_depot.client.DyeDepotClient"
    ],
    "fabric-datagen": [
      "com.ninni.dye_depot.data.DyeDepotDatagen"
    ],
    "jei_mod_plugin": [
      "com.ninni.dye_depot.compat.DyeDepotJei"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17",
    "fabric-api": "*",
    "minecraft": "~1.21",
    "java": ">=21"
  }
}
